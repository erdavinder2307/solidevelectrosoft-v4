{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/api/*",
      "/assets/*",
      "/src/assets/*",
      "/*.{css,js,map}",
      "/*.{png,jpg,jpeg,gif,svg,webp,ico}",
      "/*.{woff,woff2,ttf,otf}"
    ]
  },
  "globalHeaders": {
    "X-Frame-Options": "DENY",
    "X-Content-Type-Options": "nosniff",
    "Referrer-Policy": "strict-origin-when-cross-origin"
  }
}
